<h2>Add Department</h2>

<% form_for(@child) do |f| %>
  <%= f.error_messages %>
  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :department_id %><br />
    <%= f.text_field :department_id %>
  </div>

  <div class="actions">
    <%= f.submit "Add Child" %>
  </div>
<% end %>
