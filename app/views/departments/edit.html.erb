<h2>Edit Department</h2>

<% form_for(@department) do |f| %>
  <%= f.error_messages %>
  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :img_uri %><br />
    <%= f.text_field :img_uri %>
  </div>
 
  <div class="actions">
    <%= f.submit "Update" %>
  </div>
<% end %>


